{
  "name": "availity-angular",
  "private": true,
  "description": "Availity Angular Components",
  "version": "3.0.0-alpha.0",
  "main": "lib/index.js",
  "keywords": [
    "angular",
    "availity",
    "sdk"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "bugs": {
    "url": "https://github.com/Availity/availity-angular/issues"
  },
  "scripts": {
    "start": "npm start --prefix packages/docs"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:Availity/availity-angular.git"
  },
  "homepage": "http://availity.github.io/availity-angular/",
  "author": "Robert McGuinness <rob.mcguinness@availity.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        85
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-availity": "^4.0.0-alpha.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3"
  }
}
