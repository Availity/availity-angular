{
  "name": "availity-angular",
  "description": "Availity Angular SDK",
  "version": "2.0.0-beta.0",
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=development node dev/index.js -c start",
    "build:lib": "copyup 'src/**/*.html' 'src/**/*.less' lib && babel src --out-dir lib --quiet --ignore specs.js,'**/tests/**','**/test/**','**/bower_components/**','**/docs/**'",
    "build:umd": "cross-env NODE_ENV=production node dev/index.js -c build",
    "build": "npm run build:umd && npm run build:lib",
    "clean": "node dev/index.js -c clean",
    "docs": "cross-env NODE_ENV=development node dev/index.js -c docs",
    "test": "karma start ./karma.conf.js",
    "test:debug": "karma start ./karma.conf-debug.js",
    "test:ci": "karma start ./karma.conf-sauce-labs.js",
    "release": "node dev/index.js -c release",
    "prepublish": "in-publish && npm run build || true",
    "lint": "node dev/index.js -c lint",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:Availity/availity-angular.git"
  },
  "files": [
    "README.md",
    "lib",
    "dist",
    "less"
  ],
  "author": "Robert McGuinness <rob.mcguinness@availity.com>",
